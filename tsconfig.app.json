{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "lib": ["ES2021", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2021"
  },
  "include": ["src/**/*", "index.html"],
  "exclude": ["node_modules", "dist"]
}


